{
  "ConnectionStrings": {
    "ProviderName": "System.Data.SqlClient"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "CorsOrigins": [ "*", "https://localhost:3000", "http://localhost:3000", "http://127.0.0.1:3000" ],
  "SessionStateMode": "#{SessionStateMode}#",
  "CustomProvider": "#{CustomProvider}#",
  "SessionStateProvider": {
    "Name": "ASPNETSessionStateStore",
    "Type": "Microsoft.Web.Redis.RedisSessionStateProvider",
    "Host": "#{SessionStateHost}#",
    "AccessKey": "",
    "Ssl": "false"
  }
}
